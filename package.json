{
  "name": "cell-bitset",
  "version": "0.0.0",
  "description": "Fast JS BitSet implementation. No worrying about 32bits restrictions.",
  "main": "src/BitSet.js",
  "scripts": {
    "docs": "./node_modules/.bin/jsdoc src -r -d doc",
    "minify": "uglifyjs src/BitSet.js --compress --mangle --output bin/BitSet.min.js",
    "test": "karma start --single-run",
    "update": "npm-check-updates -u && npm install"
  },
  "repository": {
    "type": "git",
    "url": "github.com/unnoon/cell-bitset"
  },
  "keywords": [
    "bitset",
    "bitvector",
    "bitarray",
    "bitstring",
    "fast"
  ],
  "author": "Rogier Geertzema",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.11.9",
    "jsdoc": "^3.4.0",
    "karma": "~1.1.0",
    "karma-chai-plugins": "~0.7.0",
    "karma-chrome-launcher": "~1.0.1",
    "karma-coverage": "~1.0.0",
    "karma-coveralls": "~1.1.2",
    "karma-firefox-launcher": "~1.0.0",
    "karma-ie-launcher": "~1.0.0",
    "karma-mocha": "~1.1.1",
    "karma-phantomjs-launcher": "~1.0.0",
    "karma-requirejs": "~1.0.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "~2.1.4",
    "requirejs": "^2.2.0",
    "uglify-js": "^2.6.4"
  }
}
